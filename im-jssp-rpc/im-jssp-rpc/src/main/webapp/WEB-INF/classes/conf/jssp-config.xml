<intra-mart>
	<server-character-encoding>UTF-8</server-character-encoding>
	<jssp-rpc>
		<key>
			<uri-suffix>.jssprpc</uri-suffix>
		</key>					
		<marshller>
			<page-path>jssp_rpc/common/json</page-path>
			<throw-unmarshall-exception>true</throw-unmarshall-exception>
			<function-name>
				<unmarshall>unmarshall</unmarshall>													
				<marshall>marshall</marshall>
				<marshall-arguments>marshall4Arguments</marshall-arguments>
			</function-name>
		</marshller>		
	</jssp-rpc>
	<jssp>
		<key>
			<uri-suffix>.jssp</uri-suffix>
		</key>
		
		<java-script-api>
			<api-class>org.intra_mart.jssp.view.tag.ImartObject</api-class>
			<api-class>org.intra_mart.jssp.script.api.DebugObject</api-class>
			<api-class>org.intra_mart.jssp.script.api.SessionObject</api-class>
			<api-class>org.intra_mart.jssp.script.api.FileAccessObject</api-class>
			<api-class>org.intra_mart.jssp.script.api.WebObject</api-class>
			<api-class>org.intra_mart.jssp.script.api.dom.DOMNodeObject</api-class>
			<api-class>org.intra_mart.jssp.script.api.dom.XMLParserObject</api-class>
			<api-class>org.intra_mart.jssp.script.api.dom.XMLDocumentObject</api-class>
			<api-class>org.intra_mart.jssp.script.api.URLObject</api-class>
			<api-class>org.intra_mart.jssp.script.api.ConstantObject</api-class>
			<api-class>org.intra_mart.jssp.script.api.ProcedureObject</api-class>
			<api-class>org.intra_mart.jssp.script.api.FormatObject</api-class>
			<api-class>org.intra_mart.jssp.script.api.PageObject</api-class>
		</java-script-api>
		
		<jssp-tag>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4String</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4Number</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4Date</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4Repeat</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4Condition</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4Decision</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4ContentType</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4HttpEquiv</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4Link</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4Form</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4Frame</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4Include</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4Submit</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4Input</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4Hidden</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4Select</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4Textarea</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4Tag</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4Loop</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4Event</tag-class>
			<tag-class>org.intra_mart.jssp.view.tag.ImartTag4JsspRpc</tag-class>
		</jssp-tag>
	</jssp>
</intra-mart>
